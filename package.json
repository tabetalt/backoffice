{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@tabetalt/kit": "^1.8.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "apollo-upload-client": "^14.1.2",
    "framer-motion": "^2.6.15",
    "graphql": "^15.3.0",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "3.4.3",
    "react-table": "^7.5.1",
    "theme-ui": "^0.4.0-rc.5",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "test:ci": "yarn test --watchAll=false",
    "lint": "eslint \"src/**/*.ts{,x}\"",
    "eject": "react-scripts eject",
    "pretty": "prettier --write ./src",
    "precodegen": "apollo schema:download --endpoint=http://localhost:3000/graphql",
    "codegen": "apollo codegen:generate src/api/types --addTypename --target=typescript --outputFlat --localSchemaFile=schema.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "husky": "^4.3.0",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test:ci"
    }
  }
}
